<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0" />
    <title>@ViewData["Title"] - Pacific Sunscapes Premier Landscaping</title>

    <meta name="description" content="San Diego's premier landscaping company specializing in landscape design, landscape construction, irrigation installation and maintence. Transform your outdoor living space, from lush green to dryscape solutions!" />
    <meta property="og:url" content="#" />
    <link rel="canonical" content="#" />
    <meta property="og:title" content="San Diego's Premier Landscaping Company">

    <link rel="stylesheet" href="https://cdn.jsdelivr.net/npm/bootstrap-icons@1.10.5/font/bootstrap-icons.css">

    <link rel="preconnect" href="https://fonts.googleapis.com">
    <link rel="preconnect" href="https://fonts.gstatic.com" crossorigin>
    <link href="https://fonts.googleapis.com/css2?family=Lora:wght@400;700&display=swap" rel="stylesheet">
    <link href="https://cdn.jsdelivr.net/npm/bootstrap@5.3.0-alpha3/dist/css/bootstrap.min.css" rel="stylesheet" integrity="sha384-KK94CHFLLe+nY2dmCWGMq91rCGa5gtU4mk92HdvYe+M/SXH301p5ILy+dN9+nJOZ" crossorigin="anonymous">
    <link rel="stylesheet" href="https://use.fontawesome.com/releases/v5.15.4/css/all.css">
    <link href="_content/Blazorise/blazorise.css" rel="stylesheet" />
    <link href="_content/Blazorise.Bootstrap/blazorise.bootstrap.css" rel="stylesheet" />
    <link rel="stylesheet" href="~/css/site.css" asp-append-version="true" />
    <link rel="stylesheet" href="~/PacificSunscapesWebsite.styles.css" asp-append-version="true" />
</head>
<body>
    <header>
        <nav class="navbar navbar-expand-sm navbar-light bg-dark fixed-top">
            <button id="nav-btn" class="navbar-toggler" type="button" data-bs-toggle="collapse" data-bs-target="#navbarNav" aria-controls="navbarNav" aria-expanded="false" aria-label="Toggle navigation">
                <span class="navbar-toggler-icon"></span>
            </button>
            <div class="container-fluid">
                <a class="navbar-brand" asp-area="" asp-controller="Home" asp-action="Index">
                    <img src="https://lirp.cdn-website.com/c731753ca9f741da980d87301471461f/dms3rep/multi/opt/pacific+sunscapes+logo+only-1920w.png" class="img-fluid " />
                </a>
                <div class="navbar-collapse collapse d-sm-inline-flex justify-content-end" id="navbarNav">
                   <ul class="nav navbar-nav nav-underline">
                     <li class="nav-item">
                         <a class="nav-link" asp-area="" asp-controller="About" asp-action="Index">About Us</a>
                     </li>
                        <li class="nav-item dropdown" aria-labelledby="servicesDropdown">
                            <a class="nav-link dropdown-toggle" href="#" id="servicesDropdown" role="button" data-bs-toggle="dropdown" aria-expanded="false">Services</a>
                        <ul class="dropdown-menu" aria-labelledby="servicesDropdown">
                            <li><a class="dropdown-item" asp-area="" asp-controller="LandscapeDesign" asp-action="Index">Landscape Design</a></li>
                            <li><a class="dropdown-item" asp-area="" asp-controller="LandscapeMaintenance" asp-action="Index">Landscape Maintenance</a></li>
                        </ul>
                     </li>
                     <li class="nav-item dropdown">
                            <a class="nav-link dropdown-toggle" href="#" id="resourcesDropdown" role="button" data-bs-toggle="dropdown" aria-expanded="false">Resources</a>
                        <ul class="dropdown-menu" aria-labelledby="resourcesDropdown">
                            <li><a class="dropdown-item" asp-area="" asp-controller="PlantCare" asp-action="Index">Plant Care</a></li>
                            <li><a class="dropdown-item" asp-area="" asp-controller="Blog" asp-action="Index">Blog</a></li>
                            <li><a class="dropdown-item" asp-area="" asp-controller="LawnCare" asp-action="Index">Lawn Care</a></li>
                        </ul>
                     </li>
                     <li class="nav-item">
                        <a class="nav-link" asp-area="" asp-controller="Portfolio" asp-action="Index">Portfolio</a>
                     </li>
                     <li class="nav-item">
                        <a class="nav-link" asp-area="" asp-controller="Contact" asp-action="Index">Contact Us</a>
                     </li>
                   </ul>
               </div>
           </div>
        </nav>
    
        @RenderSection("Header", required: false)
    </header>
    <div>
        <main role="main">
            @RenderBody()
        </main>
    </div>

    <footer class="border-top">
        <div class="row">
            <div class="col-12 col-md-4 col-lg-4 text-center  text-lg-center footer-contact">
                Contact<hr />
                <div class="text-md-center">
                    <i class="bi bi-telephone-fill"></i><div class="footer-phone d-lg-flex justify-content-lg-center "> (619) 820-8064</div>
                    <a href="mailto:info@PacificSunscapesWebsite.com"><i class="bi bi-envelope-fill pb-xl-3"></i> </a><div class="d-lg-flex justify-content-lg-center">info@PacificSunscapes<br />Website.com</div>
                </div>
            </div>
            <div class="col-12 col-md-4 col-lg-4 text-center mt-3">
                Our Services <hr />
                <ul>
                    <li class="me-4">
                        Lawn Care
                    </li>
                    <li class="me-4">
                        Lawn Care
                    </li>
                    <li class="me-4">
                        Lawn Care
                    </li>
                    <li class="me-4">
                        Lawn Care
                    </li>
                </ul>
            </div>
            <div class="col-12 col-md-4 col-lg-4 text-center  text-lg-center footer-contact">
                Follow Us On Social Media  <hr />
                <div class="row socials">
                    <div class="col-4 ">
                        <a href="#" target="_blank" title="Follow on Facebook">  <i class="bi bi-facebook "></i></a>
                    </div>
                    <div class="col-4">
                        <a href="#" target="_blank" title="Follow on LinkedIn">  <i class="bi bi-linkedin "></i></a>
                    </div>
                    <div class="col-4">
                        <a href="#" target="_blank" title="Follow on YouTube">  <i class="bi bi-youtube "></i></a>
                    </div>
                </div>
            </div>

            <div class="col-12 text-center">
                <div class="mb-5">
                    &copy; <span id="copyrightYear">2021</span> Sunset Coast Partner
                </div>
            </div>
        </div>
    </footer>

    <script src="~/lib/jquery/dist/jquery.min.js"></script>
    <script src="https://cdn.jsdelivr.net/npm/bootstrap@5.3.0-alpha3/dist/js/bootstrap.bundle.min.js" integrity="sha384-ENjdO4Dr2bkBIFxQpeoTz1HIcje39Wm4jDKdf19U8gI4ddQ3GYNS7NTKfAdVQSZe" crossorigin="anonymous"></script>
    <script src="~/js/site.js" asp-append-version="true"></script>
    <script src="https://cdn.jsdelivr.net/npm/jquery@3.5.1/dist/jquery.slim.min.js" integrity="sha384-DfXdz2htPH0lsSSs5nCTpuj/zy4C+OGpamoFVy38MVBnE+IbbVYUew+OrCXaRkfj" crossorigin="anonymous"></script>
    <script src="https://cdn.jsdelivr.net/npm/popper.js@1.16.1/dist/umd/popper.min.js" integrity="sha384-9/reFTGAW83EW2RDu2S0VKaIzap3H66lZH81PoYlFhbGU+6BZp6G7niu735Sk7lN" crossorigin="anonymous"></script>
    <script src="https://cdn.jsdelivr.net/npm/bootstrap@4.6.1/dist/js/bootstrap.min.js" integrity="sha384-VHvPCCyXqtD5DqJeNxl2dtTyhF78xXNXdkwX1CZeRusQfRKp+tA7hAShOK/B/fQ2" crossorigin="anonymous"></script>
    <script>app.homepage();</script>
    @await RenderSectionAsync("Scripts", required: false)
</body>
</html>

