<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0" />
    <title>@ViewData["Title"] - Pacific Sunscapes Premier Landscaping</title>

    <meta name="description" content="San Diego's premier landscaping company specializing in landscape design, landscape construction, irrigation installation and maintence. Transform your outdoor living space, from lush green to dryscape solutions!" />
    <meta property="og:url" content="#" />
    <link rel="canonical" content="#" />
    <meta property="og:title" content="San Diego's Premier Landscaping Company">

    <link rel="stylesheet" href="https://cdn.jsdelivr.net/npm/bootstrap-icons@1.10.5/font/bootstrap-icons.css">

    <link rel="preconnect" href="https://fonts.googleapis.com">
    <link rel="preconnect" href="https://fonts.gstatic.com" crossorigin>
    <link href="https://fonts.googleapis.com/css2?family=Lora:wght@400;700&display=swap" rel="stylesheet">
    <link href="https://cdn.jsdelivr.net/npm/bootstrap@5.3.0-alpha3/dist/css/bootstrap.min.css" rel="stylesheet" integrity="sha384-KK94CHFLLe+nY2dmCWGMq91rCGa5gtU4mk92HdvYe+M/SXH301p5ILy+dN9+nJOZ" crossorigin="anonymous">
    <link rel="stylesheet" href="~/css/site.css" asp-append-version="true" />
    <link rel="stylesheet" href="~/PacificSunscapesWebsite.styles.css" asp-append-version="true" />
</head>
<body>
    <header>
        <div class="contact-header">
       <div class="row p-2">
           <div class="col-4">
                    <a class="nav-link text-dark" asp-area="" asp-controller="About" asp-action="Index">Location</a>
           </div>
                <div class="col-4">
                    <a class="nav-link text-dark" asp-area="" asp-controller="About" asp-action="Index">000-000-0000</a>
                </div>
                <div class="col-4">
                    <a class="nav-link text-dark" asp-area="" asp-controller="About" asp-action="Index">000-000-0000</a>
           </div>
       </div>
        </div>
        <nav class="navbar navbar-expand-sm navbar-toggleable-sm navbar-light bg-translucent border-bottom box-shadow">
            <div class="container-fluid">
                <a class="navbar-brand" asp-area="" asp-controller="Home" asp-action="Index">
                    <img src="https://lirp.cdn-website.com/c731753ca9f741da980d87301471461f/dms3rep/multi/opt/pacific+sunscapes+logo+only-1920w.png" class="img-fluid h-25 w-25"/>
                </a>
                <button class="navbar-toggler" type="button" data-bs-toggle="collapse" data-bs-target=".navbar-collapse" aria-controls="navbarSupportedContent"
                        aria-expanded="false" aria-label="Toggle navigation">
                    <span class="navbar-toggler-icon"></span>
                </button>
                <div class="navbar-collapse collapse d-sm-inline-flex justify-content-between">
                    <ul class="navbar-nav flex-grow-1">
                    
                    
                        <li class="nav-item">
                            <a class="nav-link text-dark" asp-area="" asp-controller="About" asp-action="Index">About Us</a>
                        </li>
                        <li class="nav-item">
                            <a class="nav-link text-dark" asp-area="" asp-controller="Services" asp-action="Index">Our Services</a>
                        </li>
                        <li class="nav-item">
                            <a class="nav-link text-dark" asp-area="" asp-controller="Portfolio" asp-action="Index">Our Portfolio</a>
                        </li>
                        <li class="nav-item">
                            <a class="nav-link text-dark" asp-area="" asp-controller="Resources" asp-action="Index">Resources</a>
                        </li>
                        <li class="nav-item">
                            <a class="nav-link text-dark" asp-area="" asp-controller="Contact" asp-action="Index">Contact Us</a>
                        </li>
                    </ul>
                </div>
            </div>
        </nav>
    </header>
    <div >
        <main role="main" class="pb-3">
            @RenderBody()
        </main>
    </div>

    <footer class="border-top">
       
            <div class="content row row-cols-1">
                <div class="col-12 col-md-4 text-center text-md-start ">
               Contact Us<hr/>
               <p>Phone: (619) 820-8064</p>
               <p>Email: info@PacificSunscapesWebsite.com</p>
                </div>
                <div class="col-12 col-md-4 text-center">
               Our Services <hr/>
               <p>Lawn Care</p>
                <p>Lawn Care</p>
                  <p>Lawn Care</p>
                <p>Lawn Care</p>
                </div>  <div class="col-12 col-md-4 text-center text-md-end mb-5 ">
                  Follow Us On Social Media <hr/>
             <div class="row">
                 <div class="col-4">
                        <a href="#" target="_blank" title="Follow on Facebook">  <i class="bi bi-facebook"></i></a>
                 </div>
                  <div class="col-4">
                        <a href="#" target="_blank" title="Follow on Facebook">  <i class="bi bi-linkedin"></i></a>
                 </div>
                    <div class="col-4">
                        <a href="#" target="_blank" title="Follow on Facebook">  <i class="bi bi-youtube"></i></a>
                    </div>
             </div>

                </div> 
                <div class="d-flex justify-content-center mb-5">
                <img src="https://lirp.cdn-website.com/c731753ca9f741da980d87301471461f/dms3rep/multi/opt/pacific+sunscapes+logo+only-1920w.png" class="img-fluid w-50 " />
            </div>
        </div>
    </footer>
    <script src="~/lib/jquery/dist/jquery.min.js"></script>
    <script src="https://cdn.jsdelivr.net/npm/bootstrap@5.3.0-alpha3/dist/js/bootstrap.bundle.min.js" integrity="sha384-ENjdO4Dr2bkBIFxQpeoTz1HIcje39Wm4jDKdf19U8gI4ddQ3GYNS7NTKfAdVQSZe" crossorigin="anonymous"></script>
    <script src="~/js/site.js" asp-append-version="true"></script>
    @await RenderSectionAsync("Scripts", required: false)
</body>
</html>
